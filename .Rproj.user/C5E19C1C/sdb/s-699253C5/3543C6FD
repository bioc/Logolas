    "contents" : "\n###########   colon letter  ######################\n\n\ncolonletter <- function(plot = FALSE, colfill=\"green\",\n                      y_pos_1 = 0.3,\n                      y_pos_2 = 0.7,\n                      x_pos = 0.5,\n                      rad = 0.1,\n                      lwd=10){\n\n  angle2 <- seq(0,2*pi,length=200)\n\n  x1 <- x_pos + rad*cos(angle2)\n  y1 <- y_pos_1 + rad*sin(angle2)\n\n  x2 <- x_pos + rad*cos(angle2)\n  y2 <- y_pos_2 + rad*sin(angle2)\n\n  x <- c(x1, x2)\n  y <- c(y1, y2)\n\n\n  id <- c(rep(1, length(x1)), rep(2, length(x2)))\n  fill <- c(colfill, colfill)\n\n  if(plot){\n    grid.newpage()\n    pushViewport(viewport(x=0.5,y=0.5,width=1, height=1,\n                          clip=TRUE))\n    grid.polygon(x, y,\n                 default.unit=\"native\",\n                 id=id,\n                 gp=gpar(fill=fill,\n                         lwd=lwd))\n  }\n  ll <- list(\"x\"= x,\n             \"y\"= y,\n             \"id\" = id,\n             \"fill\" = fill)\n  return(ll)\n}\n\n## out <- colonletter(plot = TRUE)\n",
